{
  "name": "tree-sitter-applescript",
  "version": "0.1.0",
  "description": "AppleScript grammar for tree-sitter",
  "main": "bindings/node",
  "repository": {
    "type": "git",
    "url": "https://github.com/helge/tree-sitter-applescript.git"
  },
  "license": "MIT",
  "devDependencies": {
    "tree-sitter-cli": "^0.22.0"
  },
  "scripts": {
    "generate": "tree-sitter generate",
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.applescript",
      "injection-regex": "applescript",
      "file-types": ["applescript", "scpt", "scptd"],
      "highlights": "queries/highlights.scm"
    }
  ]
}
